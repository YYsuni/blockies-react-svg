{
	"compilerOptions": {
		"strict": true,
		"target": "ES6",
		"skipLibCheck": true,
		"esModuleInterop": false,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"module": "ESNext",
		"moduleResolution": "Node",
		"isolatedModules": true,
		"jsx": "react-jsx",
		"sourceMap": true,
		"declaration": true
	}
}
